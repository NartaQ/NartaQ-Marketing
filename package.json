{
  "name": "nartaq",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run build:emails && prisma generate && next build",
    "build:emails": "tsx scripts/build-emails.ts",
    "build:emails:watch": "tsx scripts/build-emails.ts -w",
    "email:preview": "email dev --dir src/emails --port 3002",
    "build:analyze": "rm -rf .next && cross-env ANALYZE=true next build",
    "build:profile": "next build --profile",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "npm run test:smoke && npm run test:health",
    "test:smoke": "tsx scripts/smoke-tests.ts",
    "test:health": "tsx scripts/health-check.ts",
    "test:post-deployment": "tsx scripts/post-deployment-tests.ts",
    "test:emails": "tsx scripts/test-emails.ts",
    "test:azure": "tsx scripts/test-azure-storage.ts",
    "send-email": "tsx scripts/send-test-email.ts",
    "bundle:analyze": "tsx scripts/bundle-analyzer.ts",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:bundle": "next build && npx @next/bundle-analyzer",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "analyze:both": "cross-env BUNDLE_ANALYZE=both next build",
    "postinstall": "prisma generate && prisma db push",
    "vercel-build": "prisma generate && next build",
    "sanity:dev": "sanity dev",
    "sanity:start": "sanity start",
    "sanity:build": "sanity build",
    "sanity:deploy": "sanity deploy",
    "sanity:deploy-graphql": "sanity graphql deploy"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.28.0",
    "@hookform/resolvers": "^5.2.1",
    "@portabletext/react": "^4.0.3",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@rive-app/react-canvas": "^4.23.1",
    "@sanity/client": "^7.11.0",
    "@sanity/image-url": "^1.2.0",
    "@sanity/vision": "^4.6.1",
    "@sendgrid/mail": "^8.1.6",
    "@tabler/icons-react": "^3.34.1",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/gsap": "^1.20.2",
    "@types/nodemailer": "^7.0.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "gsap": "^3.13.0",
    "handlebars": "^4.7.8",
    "lenis": "^1.3.8",
    "lucide-react": "^0.539.0",
    "motion": "^12.23.12",
    "next": "15.4.6",
    "next-sanity": "^10.0.16",
    "next-seo": "^6.8.0",
    "nodemailer": "^7.0.10",
    "react-email": "4.2.12",
    "posthog-js": "^1.261.3",
    "posthog-node": "^5.8.1",
    "react-hook-form": "^7.62.0",
    "sanity": "^4.6.1",
